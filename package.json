{
  "name": "ai-test-helper",
  "displayName": "AI Test Helper",
  "description": "Build/Improve Unit Test By AI",
  "publisher": "TheoSun",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Testing"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "permissions": [
    "secrets"
  ],
  "contributes": {
    "commands": [
      {
        "command": "TheoSun-ai-test-helper.set-api-key",
        "title": "AI: Set API Key"
      },
      {
        "command": "TheoSun-ai-test-helper.set-base-url",
        "title": "AI: Set API Base Url"
      },
      {
        "command": "TheoSun-ai-test-helper.create-unit-test",
        "title": "AI: Create Unit Test",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "TheoSun-ai-test-helper.create-unit-test-for-symbol",
        "title": "AI: Create Unit Test For Symbol",
        "when": "editorTextFocus && !editorReadonly"
      }
    ],
    "configuration": {
      "title": "AI Unit Test Helper Configuration",
      "properties": {
        "theosun.ai.unit.test.helper.model": {
          "type": "string",
          "default": "gpt-3.5-turbo-0301",
          "enum": [
            "gpt-4",
            "gpt-4-0314",
            "gpt-4-32k",
            "gpt-4-32k-0314",
            "gpt-3.5-turbo",
            "gpt-3.5-turbo-0301"
          ],
          "description": "OpenAI Model Name"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts --fix",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "antlr4": "^4.12.0",
    "openai": "^3.2.1"
  },
  "license": "MIT"
}
